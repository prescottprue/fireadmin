{
  "name": "fireadmin",
  "version": "0.8.4",
  "description": "Application for Managing Firebase Applications. Includes support for multiple environments and data migrations.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "size": "size-limit",
    "compile": "node build/scripts/compile",
    "build": "yarn clean && cross-env NODE_ENV=production SIZE=true yarn compile",
    "build:config": "firebase-ci createConfig",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "deploy": "firebase-ci deploy",
    "deploy:functions": "firebase deploy --only functions",
    "start": "cross-env NODE_ENV=development webpack-dashboard -p 1334 -c cyan -m -t fireadmin -- node build/scripts/start",
    "start:simple": "cross-env NODE_ENV=development node build/scripts/start",
    "start:dist": "yarn build && firebase serve --only hosting -p 3000",
    "docs:start": "yarn --cwd docs start",
    "docs:build": "yarn --cwd docs build",
    "functions:start": "yarn --cwd functions start",
    "functions:build": "yarn --cwd functions build",
    "functions:watch": "yarn --cwd functions watch",
    "functions:test": "yarn --cwd functions test",
    "functions:test:cov": "yarn --cwd functions test:cov",
    "lint": "yarn lint:app && yarn lint:functions && yarn lint:tests && yarn lint:docs",
    "lint:functions": "eslint functions",
    "lint:app": "eslint src build server project.config.js",
    "lint:tests": "eslint cypress",
    "lint:docs": "eslint docs",
    "lint:fix": "yarn lint --fix",
    "test:ui": "yarn build:testConfig && cypress run",
    "test:ui:open": "yarn build:testConfig && cypress open",
    "test:ui:stage": "yarn test:ui --env envName=stage",
    "test:ui:open:stage": "yarn test:ui:open --env envName=stage",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/fireadmin.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.32.0",
    "date-fns": "^1.29.0",
    "firebase": "^5.8.2",
    "firepad": "1.4.0",
    "lodash": "^4.17.11",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "promise": "^7.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.8.1",
    "react-dropzone": "^4.2.1",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.5.3",
    "react-instantsearch": "^4.2.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.6",
    "react-router": "^3.0.0",
    "recompose": "^0.30.0",
    "redbox-react": "^1.3.6",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-firestore": "^0.5.7",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
