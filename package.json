{
  "name": "fireadmin",
  "version": "0.5.9",
  "description": "Application for Managing Firebase Applications. Includes support for multiple environments and data migrations.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "build:functions": "npm run build --prefix functions",
    "build:config": "firebase-ci createConfig",
    "build:testConfig": "$(npm bin)/babel-node build/ci/createTestEnvFile",
    "deploy": "firebase-ci deploy",
    "start": "cross-env NODE_ENV=development webpack-dashboard -p 1334 -c cyan -m -t fireadmin -- node build/scripts/start",
    "start:simple": "cross-env NODE_ENV=development node build/scripts/start",
    "functions:start": "npm run start --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:test": "npm run test --prefix functions",
    "lint": "eslint src functions/src functions/test build server project.config.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build:testConfig && cross-env CYPRESS_baseUrl=http://localhost:3000 cypress run",
    "test:stage": "npm run build:testConfig && cross-env CYPRESS_baseUrl=https://fireadmin-stage.firebaseapp.com cypress run",
    "test:ui:stage": "npm run build:testConfig && cross-env CYPRESS_baseUrl=https://fireadmin-stage.firebaseapp.com $(npm bin)/cypress open",
    "test:ui": "npm run build:testConfig && cross-env CYPRESS_baseUrl=http://localhost:3000 $(npm bin)/cypress open -- --browser chrome",
    "test:ui:debug": "npm run build:testConfig && cross-env CYPRESS_baseUrl=http://localhost:3000 DEBUG=cypress:* cypress open",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/fireadmin.git"
  },
  "engines": {
    "node": "8"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "1.4.0",
    "@material-ui/icons": "1.0.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.32.0",
    "date-fns": "^1.29.0",
    "firebase": "^5.0.3",
    "firepad": "^1.4.0",
    "lodash": "^4.17.10",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "promise": "^7.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.3.0",
    "react-dropzone": "^4.2.1",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.3.0",
    "react-instantsearch": "^4.2.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.6",
    "react-router": "^3.0.0",
    "recompose": "^0.27.1",
    "redbox-react": "^1.3.6",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-firestore": "^0.5.7",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "cypress": "3.0.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "expose-loader": "^0.7.4",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "figures": "^2.0.0",
    "file-loader": "^0.11.1",
    "firebase-admin": "^5.13.0",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "ip": "^1.1.5",
    "node-sass": "^4.5.3",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.13.2",
    "yargs": "^12.0.0"
  }
}
