    "firebase-tools": "^6.4.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^1.3.1",
    "ip": "^1.1.5",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.5.3",
    "prettier": "^1.15.3",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "size-limit": "^0.21.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.13.2",
    "yargs": "^12.0.0"
  },
  "husky": {
